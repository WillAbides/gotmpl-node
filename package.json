{
  "name": "gotmpl",
  "version": "0.1.0",
  "description": "A wrapper for https://github.com/willabides/gotmpl",
  "repository": "github.com/willabides/gotmpl-node",
  "license": "MIT",
  "main": "build/src/index.js",
  "engines": {
    "node": ">=16"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "arch": [
    "x64",
    "arm64",
    "ia32"
  ],
  "files":[
    "build/**/*",
    "script/*",
    ".bindown.yml"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "chai": "^4.3.7",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "protoc-gen-js": "^3.21.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "test": "npm run clean && npm run compile && npm run install-gotmpl && mocha build/src/*.test.js",
    "install-gotmpl": "script/bindown install gotmpl && mkdir -p build/bin && cp -a bin/gotmpl build/bin/gotmpl",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "postinstall": "npm run install-gotmpl"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.2",
    "@protobuf-ts/runtime-rpc": "^2.8.2",
    "nice-grpc": "^2.1.0",
    "protobufjs": "^7.2.2"
  }
}
